{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/api/items",
      "methods": ["GET"],
      "dest": "/index.js"
    },
    {
      "src": "/api/items/:id",
      "methods": ["GET"],
      "dest": "/index.js"
    }
  ]
}
